{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/(frontend)/communities/[...slug]/page.tsx"
, "app/(frontend)/components/Property/MortgageCalculator.js", "app/(frontend)/components/UI/ReactSelect/MultiValueContainer.js", "app/(frontend)/components/UI/CustomSelectToggle.js", "app/(frontend)/components/Project/Filters.js", "app/pages/_document.js"  ],
  "exclude": ["node_modules"]
}
